<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net9.0</TargetFrameworks>
        <Nullable>enable</Nullable>
        <Platforms>AnyCPU</Platforms>
        <NoWarn>$(NoWarn);1591</NoWarn>
    </PropertyGroup>

    <PropertyGroup>
        <Description>A concoction of Azure Blob Storage utilities and libraries</Description>
        <Version Condition="'$(BUILD_VERSION)' == ''">3.0.0</Version>
        <Version Condition="'$(BUILD_VERSION)' != ''">$(BUILD_VERSION)</Version>
        <AssemblyName>Soenneker.Blob.Suite</AssemblyName>
        <PackageId>Soenneker.Blob.Suite</PackageId>
        <PackageTags>azure blob suite c# .net dotnet</PackageTags>
        <Copyright>Copyright Â© 2023-2025 Jake Soenneker</Copyright>
        <Authors>Jake Soenneker</Authors>
        <Company>https://soenneker.com</Company>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <EmbedUntrackedSources>true</EmbedUntrackedSources>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <IsPackable>true</IsPackable>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/soenneker/soenneker.blob.suite</PackageProjectUrl>
        <RepositoryUrl>https://github.com/soenneker/soenneker.blob.suite</RepositoryUrl>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <LangVersion>latest</LangVersion>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageIcon>icon.png</PackageIcon>
    </PropertyGroup>
    
    <ItemGroup>
        <None Include="..\README.md" Pack="true" PackagePath="\" />
        <None Include="..\LICENSE" Pack="true" PackagePath="\" />
        <None Include="..\icon.png" Pack="true" PackagePath="\" />

        

        <PackageReference Include="Soenneker.Blob.Copy" Version="3.0.942" />

        <PackageReference Include="Soenneker.Blob.Delete" Version="3.0.2473" />

        <PackageReference Include="Soenneker.Blob.Download" Version="3.0.2890" />

        <PackageReference Include="Soenneker.Blob.Service" Version="3.0.2119" />

        <PackageReference Include="Soenneker.Blob.Upload" Version="3.0.2685" />
    </ItemGroup>
</Project>
